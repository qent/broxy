    {
      "icon": "exa",
      "allOf": [
        { "field": "transport", "pattern": "^streamable-http$" },
        { "field": "url", "pattern": "^https://mcp\\.exa\\.ai/mcp$" }
      ]
    },
    {
      "icon": "jetbrains",
      "allOf": [
        { "field": "transport", "pattern": "^http$" },
        { "field": "headers.key", "pattern": "^IJ_MCP_SERVER_PROJECT_PATH$" }
      ]
    },
    {
      "icon": "notion",
      "allOf": [
        { "field": "transport", "pattern": "^streamable-http$" },
        { "field": "url", "pattern": "^https://mcp\\.notion\\.com/mcp$" }
